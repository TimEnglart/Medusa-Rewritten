		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"@types/express": "^4.17.7",
		"@types/node": "^12.12.48",
		"@types/ws": "^6.0.4",
		"@types/xml2js": "^0.4.5",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
